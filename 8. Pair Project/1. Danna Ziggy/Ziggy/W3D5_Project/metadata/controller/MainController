                    "var price, bestseller;",
                    "if(Ext.isEmpty(this.orderWin)){",
                    "    this.orderWin = Ext.create('W3D5_Project.view.OrderWin', {});",
                    "}",
                    "this.orderWin.show();",
                    "this.orderWin.unmask();",
                    "if(record.data.avail>0){",
